$(function() {
  <% @json_feature_result.first['elements'].each do |element| %>
    <% element['steps'].each do |step| %>
      <% status = step['result']['status'] %>
      $("#<%= step['code'] %>").html('<%= t("results.#{status}") || status %>');
    <% end %>
  <% end %>

  $("#run_feature_link").html("Executar feature");
 
});